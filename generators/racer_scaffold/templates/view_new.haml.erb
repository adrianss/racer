%h2= t('activerecord.models.<%= singular_name %>.one', :default => '<%= singular_name %>')

= error_messages_for :<%= singular_name %>

- form_for(@<%= singular_name %>, :builder => DynaFormBuilder) do |f|
  = render :partial => "form", :locals => { :f => f }
  .form-controls= f.submit t('create', :default => "Guardar")

- content_for :local_menu do
  = render :partial => 'shared/local_menu'
